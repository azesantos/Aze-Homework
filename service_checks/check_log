#/bin/bash
# by Aze Santos

LOGFILE=('/tmp/daemon_print.log')

PARSELOG()
{
tail -1 $LOGFILE | grep -wo -e OK -e WARNING -e CRITICAL -e UNKNOWN
}

OUTPUT=$(PARSELOG)

if [ $OUTPUT == OK ];
  then exit 0
elif [ $OUTPUT == WARNING ];
  then exit 1
elif [ $OUTPUT == CRITICAL ];
  then exit 2
else
  exit 3
fi
